          "name": "Scout",
          "desc": "20cm formation coherency. 10cm zone of control.",
          "ref": "2.1.12"
        },
        "skimmer": {
          "name": "Skimmer",
          "desc": "Ignore terrain and friendly units while moving. Can pop up over terrain (closer to the skimmer) for shooting (except on sustained fire and marshall actions). May always chose firefight in assault.",
          "ref": "2.1.13"
        },
        "sniper": {
          "name": "Sniper",
          "desc": "Roll for snipers separately. The attacker may chose which enemy unit is hit. -1 to save throws.",
          "ref": "2.1.14"
        },
        "supreme-commander": {
          "name": "Supreme Commander",
          "desc": "Re-roll initiative once per turn. Remove an extra blast marker when it regroups or rallies. Up to 3 formations may join when launching an assault.",
          "ref": "2.1.15"
        },
        "thick-rear-armour": {
          "name": "Thick Rear Armour",
          "desc": "Ignore the -1 save on crossfire.",
          "ref": "2.1.16"
        },
        "teleport": {
          "name": "Teleport",
          "desc": "Place unit in any turn before strategy roll. Roll d6 for each teleporting unit, with a 1 you receive a blast marker.",
          "ref": "2.1.17"
        },
        "walker": {
          "name": "Walker",
          "desc": "Re-roll dangerous terrain tests.",
          "ref": "2.1.18"
        },
        "slow-and-steady": {
          "name": "Slow and Steady",
          "desc": "They may not be used on the first two turns.",
          "ref": "2.1.19"
        },
        "support-craft": {
          "name": "Support Craft",
          "desc": "Ignore terrain and friendly units while moving. Can be seen over terrain (closer to the support craft). Always uses firefight in assault.",
          "ref": "2.1.20"
        },
        "self-planetfall": {
          "name": "Self Planetfall",
          "desc": "Can planetfall even if an enemy spacecraft is present.",
          "ref": "2.1.21"
        },
        "expendable": {
          "name": "Expendable",
          "desc": "A formation does not receive a Blast marker when a unit with expendable is destroyed.",
          "ref": "2.1.22"
        },
        "tunneler": {
          "name": "Expendable",
          "desc": "Select arrival zone before game and at which turn (opponents half needs turn 3 or more). Place units before strategy roll.",
          "ref": "2.1.23"
        }
      },
